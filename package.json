{
  "name": "vanilla",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ui-library": "ng build --prod --project ui-library",
    "build:ui-library-elements": "ng build --prod --project ui-library-elements --output-hashing none && npm run build:ui-library-elements-files",
    "build:ui-library-elements-files": "node build-ui-library-elements-files.js",
    "build:ui": "ng build --prod --project ui",
    "build:ui-elements": "ng build --prod --project ui-elements --output-hashing none && npm run build:ui-elements-files",
    "build:ui-elements-files": "node build-ui-elements-files.js",
    "server:mock": "nodemon ./projects/ui-elements/src/server/server.js",
    "start:dev": "ng serve --configuration=mock",
    "start:mock": "concurrently \"npm run server:mock\" \"npm run start:dev\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/elements": "^9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "concat": "^1.0.3",
    "concat-glob-cli": "^0.1.0",
    "custom-event-polyfill": "^1.0.7",
    "document-register-element": "^1.7.2",
    "fs-extra": "^9.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "nodemon": "^2.0.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
